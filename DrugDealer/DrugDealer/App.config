<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="rebus" type="Rebus.Configuration.RebusConfigurationSection, Rebus" />
  </configSections>

  <appSettings>
    <add key="mongo" value="mongodb://localhost/codeCamp"/>
  </appSettings>
  
  <rebus inputQueue="drugDealer.input" errorQueue="drugDealer.error" workers="1" maxRetries="5">
    <endpoints>
      <!-- <add messages="Name.Of.Assembly" endpoint="message_owner_1"/> -->
    </endpoints>
  </rebus>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>